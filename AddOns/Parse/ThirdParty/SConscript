import get_parse_libs, os, os.path

Import('WaliDir')
Import('LibInstallDir')

(XercesDir,libs) = get_parse_libs.getlibs(LibInstallDir)
Export('XercesDir')

xerces = os.path.join(WaliDir,'AddOns','Parse','ThirdParty',XercesDir)

built = [File(x) for x in libs]
Return('built')

