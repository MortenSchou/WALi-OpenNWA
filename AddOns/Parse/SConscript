Import('BaseEnv')

built = []

xerces = SConscript('ThirdParty/SConscript')
built += xerces

parse = SConscript('Source/SConscript', build_dir='_build',duplicate=0)
built += parse

examples = SConscript('Examples/SConscript', build_dir='_build/Examples',duplicate=0)
built += examples

BaseEnv.Depends(parse,xerces)

Return('built')
