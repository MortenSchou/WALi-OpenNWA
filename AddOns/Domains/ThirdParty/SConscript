## ########################################
## Runs get_domains_libs.getlibs.
## (I.e., download and compile buddy-2.4)
## $Id$
##

import os
import os.path
import get_domains_libs


Import('WaliDir')
Import('LibInstallDir')

get_domains_libs.getlibs(LibInstallDir)

buddy = os.path.join(WaliDir,'AddOns','Domains','ThirdParty','buddy-2.4')

built = [Dir(buddy)]

Return('built')

