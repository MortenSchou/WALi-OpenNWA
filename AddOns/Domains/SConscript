## ##################################
## Build libdomains
## $Id$
import os,os.path

Import('BaseEnv')
Import('BuildDir')
Import('Is64')

built = []

buddy = SConscript('ThirdParty/SConscript')
built += buddy

domains = SConscript(
    'Source/SConscript', 
    build_dir=os.path.join(BuildDir,'AddOns','Domains'),
    duplicate=0)

built += domains

BaseEnv.Depends(domains,buddy)

Return('built')

